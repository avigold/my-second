# Copy this to .env and fill in real values.
# .env is gitignored — never commit secrets.

# PostgreSQL connection string (required)
DATABASE_URL=postgresql://mysecond:mysecond@localhost:5433/mysecond

# Flask session signing key — generate with: python -c "import secrets; print(secrets.token_hex(32))"
# Required in production. In dev, a random key is generated on each restart (sessions won't persist).
FLASK_SECRET_KEY=

# Anthropic API key — used by the Strategise feature
ANTHROPIC_API_KEY=

# Path to Stockfish binary (optional — auto-detected via `which stockfish` if not set)
# MYSECOND_STOCKFISH_PATH=/usr/games/stockfish
